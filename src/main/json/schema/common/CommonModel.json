{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Common model Details Embedded MongoDB Collection",
  "title": "Common model Details",
  "type": "object",
    "properties": {
      "id": {
        "description": "Unique Identifier for each record",
        "type":"string"
      },
      "createdBy": {
        "description": "Record Created by the User",
        "type":"string"
      },
      "createdDate": {
        "description": "Record Created by the User",
        "type": "string",
        "format": "date-time"
      },
      "updatedBy": {
        "description": "Record last Updated by the User",
        "type":"string"
      },
      "updatedDate": {
        "description": "Record last Updated date",
        "type": "string",
        "format": "date-time"
      },
      "status": {
        "description": "Current record status",
        "type":"string"
      }
  }
}