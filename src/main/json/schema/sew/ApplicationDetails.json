{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Reusable embed schema",
  "title": "Application and SubApplication ID for reuse in other schema",
  "type": "Object",
  "additionalItems": true,
  "uniqueItems": false,
  "properties": {
    "applicationId": {
      "type": "string",
      "description": "The Onboarded Application id"
    },
    "subApplicationId": {
        "type": "string",
        "description": "The Onboarded Sub Application id"
      }
  },
  "required": [ "applicationId","subApplicationId" ]
}