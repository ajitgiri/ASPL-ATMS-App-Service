{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Audit History Embedded MongoDB Collection",
  "title": "Scheduler Lock",
  "type": "object",
  "additionalItems": true,
  "uniqueItems": false,
    "properties": {
      "id": {
        "description": "schedule task name",
        "type":"string"
      },
      "lockedBy": {
        "description": "Audit history Updated User",
        "type":"string"
      },
      "lockedAt": {
        "description": "Audit history Updated time stamp",
        "type": "string",
        "format": "date-time"
      },
      "lockedUntil": {
        "description": "Audit history Updated time stamp",
        "type": "string",
        "format": "date-time"
      },
      "jobStatus": {
        "description": "Job Execution status : COMPLETED/FAILED",
        "type":"string"
      }
  },
  "required": [ "id", "lockedBy", "lockedAt", "lockedUntil" ,"jobStatus"]
}